#!/usr/bin/env php
<?php
require __DIR__.'/../vendor/autoload.php';
use Symfony\Component\Console\Application;
date_default_timezone_set('PRC');
$dotenv = new Dotenv\Dotenv(__DIR__.'/../');
$dotenv->load();

$app = new Application();
#TIP 命令项
$commands = [
    \App\Command\CreateCommand::class,
    \App\Command\Client::class,
    \App\Command\TcpServer::class,
    \App\Command\PipeServer::class,
    \App\Command\PipeClient::class,
];
foreach($commands as $command){
    $app->add(new $command);
}

$app->run();
